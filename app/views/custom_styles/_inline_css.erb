<style>
  <% if CustomStyle.current.logo.present? %>
    #logo .home-link {
      margin: 0px;
      width: 230px;
      height: 55px;
      background-image: url("<%= custom_styles_logo_path(digest: CustomStyle.current.digest, filename: CustomStyle.current.logo_identifier) %>");
      background-repeat: no-repeat;
      background-position-x: 0px;
      background-position-y: 0px;
      background-size: cover;
    }
  <% end %>
</style>
